#$ -S /bin/bash

for alg in `seq 0 9` ; do
    python ga-svm.py 1 1        > out100100.$alg &
    python ga-svm.py 0.05 0.95  > out0595.$alg &
    python ga-svm.py 0.1 0.9    > out1090.$alg &
    python ga-svm.py 0.15 0.85  > out1585.$alg;
    python ga-svm.py 0.2 0.8    > out2080.$alg &
    python ga-svm.py 0.25 0.85  > out2585.$alg &
    python ga-svm.py 0.3 0.7    > out3070.$alg &
    python ga-svm.py 0.5 0.8    > out5080.$alg;
done
